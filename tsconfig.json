{
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "noImplicitAny": true,
    "allowJs": true,
    "noEmit": true,
    "noUnusedLocals": true,
    "resolveJsonModule": true,
    "types": [
      "nuxt",
      "@nuxt/types",
      "@types/node",
      "quasar",
      "kakao.maps.d.ts",
      "@types/daum-postcode",
      "@vite-pwa/nuxt",
      "node"
    ],
    "baseUrl": ".",
    "paths": {
      "~/*": ["./*"],
      "@/*": ["./*"],
      "#internal/nitro": ["./node_modules/nitropack/dist/runtime"],
      "#internal/nitro/app": ["./node_modules/nitropack/dist/runtime/app"],
      "#internal/nitro/utils": ["./node_modules/nitropack/dist/runtime/utils"],
      "#vue-router": ["./node_modules/vue-router"]
    }
  },
  "exclude": [
    "**/dist/**",
    "**/.nuxt/**",
    "**/nuxt.d.ts",
    "**/examples/**",
    "**/docs/**",
    "**/playground/**",
    "**/test/fixtures/**",
    "test/nuxt/**"
  ]
}
